<?xml version="1.0" encoding="utf-8" ?>
<Trebuchet>
  <Framework>
    <Components>
      <Component
        Namespace = "Trebuchet.Framework.LOG" 
        Enabled   = "True" 
        Type      = "Static"> 
        <Console 
          Title = "Trebuchet" />
        <Header 
          Color    = "Cyan" 
          Designer = "[@time] [@type]" />
        <Body 
          Color = "White" />
      </Component>
      <Component 
        Namespace = "Trebuchet.Components.Network.NetworkComponent"
        Enabled   = "True" 
        Type      = "Dynamic">
        <Binding 
          IP   = "192.168.2.2" 
          Port = "30000" />
        <Listening 
          Backlog = "4" />
        <Async
          Receive = "10000" 
          Send    = "10000" 
          Accept  = "10000" />
      </Component>
    </Components>
  </Framework>
</Trebuchet>

